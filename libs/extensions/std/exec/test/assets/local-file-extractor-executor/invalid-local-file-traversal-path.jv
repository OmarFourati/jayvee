pipeline TESTLOCAL{

    block localextractortest oftype LocalFileExtractor {
        filePath: "./local-file-extractor-executor/local-file-test.csv";
    }

    block localfilextrctor oftype TextFileInterpreter {

	}

	block BatteryMaterialsCSVInterpreter oftype CSVInterpreter {
		delimiter: ",";
        enclosing: '"';
	}

    block test oftype TableInterpreter{
        header: true;
        columns: [
            "HeaderExample1" oftype text
        ];
    }

    block test2 oftype SQLiteLoader{
        table: "TestLocalFile";
        file: "./testlocal.sqlite";
    }


    localextractortest
    -> localfilextrctor
    -> BatteryMaterialsCSVInterpreter
    -> test
    -> test2;

}